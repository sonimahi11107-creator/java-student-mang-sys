public class Student{


    private int id;
    private String name;
    private String fname;
    private String m_name;
    private String add;



    public Student(int id, String name, String fname , String m_name, String add) {
        this.id = id;
        this.name = name;
        this.fname = fname;
 this.m_name = m_name;
    }
}
